            "created_at": "2025-07-18T22:35:06Z",
            "diff_hunk": "@@ -9,8 +9,20 @@\n \n local util = require 'lspconfig.util'\n \n+---@param config vim.lsp.ClientConfig\n+local function get_cmd(config)\n+  if config.root_dir == nil then\n+    return 'biome'\n+  end\n+  local local_cmd = config.root_dir .. '/node_modules/.bin/biome'\n+  return vim.fn.executable(local_cmd) == 1 and local_cmd or 'biome'\n+end\n+\n return {\n-  cmd = { 'biome', 'lsp-proxy' },\n+  cmd = function(dispatchers, config)\n+    local command = get_cmd(config)",

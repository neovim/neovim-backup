            "created_at": "2025-02-10T20:47:43Z",
            "diff_hunk": "@@ -275,6 +280,24 @@ function M.find(names, opts)\n   local stop = opts.stop\n   local limit = opts.limit or 1\n \n+  if opts.dfs then\n+    local matches = {}\n+    if type(names) == 'function' then\n+      error(\"Can't use dfs with functions\")\n+    end\n+    for _, name in ipairs(names) do\n+      opts.dfs = false\n+      local match = vim.fs.find(name, opts)",
